local Button = Tab:CreateButton({
   Name = "Noclip",
   Callback = function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local head = character:WaitForChild("Head")
local screenGui = Instance.new("ScreenGui")
local toggleButton = Instance.new("TextButton")
local uICorner = Instance.new("UICorner") -- Add UICorner for rounded corners

-- Add ScreenGui to the player's PlayerGui
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Set up toggle button properties
toggleButton.Size = UDim2.new(0, 50, 0, 50) -- Size of the button
toggleButton.Font = Enum.Font.FredokaOne
toggleButton.Position = UDim2.new(1, -125, 0, 0) -- Position on the right side of the screen
toggleButton.Text = "Noclip"
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 139) -- Dark blue color
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text color
toggleButton.Parent = screenGui

-- Add rounded corners to the button
uICorner.CornerRadius = UDim.new(0, 15) -- Adjust the radius as needed
uICorner.Parent = toggleButton

local collisionEnabled = false

-- Function to make a part uncollidable
local function makeUncollidable(part)
    part.CanCollide = false
end

-- Function to handle touches
local function onTouch(otherPart)
    if collisionEnabled then
        -- Ignore if the part is the floor the player is standing on
        if otherPart:IsDescendantOf(game.Workspace.Terrain) then
            return
        end

        -- Ignore if the part is already in process
        if not otherPart.CanCollide then
            return
        end

        -- Make the part uncollidable
        makeUncollidable(otherPart)
    end
end

-- Toggle collision on button click
toggleButton.MouseButton1Click:Connect(function()
    collisionEnabled = not collisionEnabled
    toggleButton.Text = collisionEnabled and "Status: On" or "Status: Off"
    toggleButton.TextScaled = True
end)

-- Connect the touch event to head
head.Touched:Connect(onTouch)
   end,
})

