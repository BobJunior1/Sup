local Button = Tab:CreateButton({
   Name = "Noclip",
   Callback = function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local head = character:WaitForChild("Head")
local screenGui = Instance.new("ScreenGui")
local toggleButton = Instance.new("TextButton")
local uICorner = Instance.new("UICorner") -- Add UICorner for rounded corners

-- Add ScreenGui to the player's PlayerGui
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Set up toggle button properties
toggleButton.Size = UDim2.new(0, 50, 0, 50) -- Size of the button
toggleButton.Font = Enum.Font.FredokaOne
toggleButton.Position = UDim2.new(1, -125, 0, 0) -- Position on the right side of the screen
toggleButton.Text = "Noclip"
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 0, 139) -- Dark blue color
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text color
toggleButton.Parent = screenGui

-- Add rounded corners to the button
uICorner.CornerRadius = UDim.new(0, 15) -- Adjust the radius as needed
uICorner.Parent = toggleButton

local collisionEnabled = false

-- Function to make a part uncollidable
local function makeUncollidable(part)
    part.CanCollide = false
end

-- Function to handle touches
local function onTouch(otherPart)
    if collisionEnabled then
        -- Ignore if the part is the floor the player is standing on
        if otherPart:IsDescendantOf(game.Workspace.Terrain) then
            return
        end

        -- Ignore if the part is already in process
        if not otherPart.CanCollide then
            return
        end

        -- Make the part uncollidable
        makeUncollidable(otherPart)
    end
end

-- Toggle collision on button click
toggleButton.MouseButton1Click:Connect(function()
    collisionEnabled = not collisionEnabled
    toggleButton.Text = collisionEnabled and "Status: On" or "Status: Off"
    toggleButton.TextScaled = True
end)

-- Connect the touch event to head
head.Touched:Connect(onTouch)
   end,
})

local Button = Tab:CreateButton({
   Name = "chat Trolling(ðŸ”‘)",
   Callback = function()
   loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Synergy-Networks/products/main/BetterBypasser/loader.lua"))()
   end,
})

local Button = Tab:CreateButton({
   Name = "Float(can't be turned off)",
   Callback = function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Variable to store the desired float height
_G.DesiredFloatHeight = _G.DesiredFloatHeight or 6.4 -- Default to 6 if not already set

-- Function to ensure zero gravity
local function ensureZeroGravity()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        -- Remove any existing BodyForce
        local existingBodyForce = humanoidRootPart:FindFirstChildOfClass("BodyForce")
        if existingBodyForce then
            existingBodyForce:Destroy()
        end

        -- Set player's gravity to zero
        local bodyForce = Instance.new("BodyForce")
        bodyForce.Force = Vector3.new(0, workspace.Gravity * humanoidRootPart.AssemblyMass, 0)
        bodyForce.Parent = humanoidRootPart
    end
end

-- Function to update the player's position instantly
local function updatePlayerPositionInstant()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        -- Perform a raycast to detect the ground below the player
        local rayOrigin = humanoidRootPart.Position
        local rayDirection = Vector3.new(0, -100, 0) -- Long enough to ensure detection
        local raycastParams = RaycastParams.new()
        raycastParams.FilterDescendantsInstances = {character}
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)

        if raycastResult then
            -- Ensure the detected part is collideable
            if raycastResult.Instance.CanCollide then
                local groundY = raycastResult.Position.Y
                local desiredY = groundY + _G.DesiredFloatHeight -- Use the desired float height

                -- Set the new position immediately
                humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position.X, desiredY, humanoidRootPart.Position.Z)
            end
        end
    end
end

-- Variable to store the connection to RenderStepped
local renderSteppedConnection

-- Function to ensure zero gravity and connect RenderStepped
local function initializeZeroGravity()
    ensureZeroGravity()
    if renderSteppedConnection then
        renderSteppedConnection:Disconnect()
    end
    renderSteppedConnection = game:GetService("RunService").RenderStepped:Connect(updatePlayerPositionInstant)
end

-- Ensure the script works when the character respawns
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    initializeZeroGravity()
end)

-- Initialize with the desired float height
initializeZeroGravity()
   end,
})
